{
    "default": {
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "publishQuiet": true,
        "dryRun": false,
        "paths": [
            "src/test/features/"
        ],
        "require": [
            "src/test/steps/*.ts",
            "src/hooks/hooks.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "html:test-results/cucumber-report.html",
            "rerun:@rerun.txt",
            "./reporter.mjs"
        ],
        "parallel": 2
    },
    "rerun": {
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "publishQuiet": true,
        "dryRun": false,
        "require": [
            "src/test/steps/*.ts",
            "src/hooks/hooks.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "rerun:@rerun.txt",
            "reporter.mjs"
        ]
    }
}